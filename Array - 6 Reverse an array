#include <iostream>
using namespace std;
int main() {
int a[100],n;
  cout<<"Enter the number of elements in the array"<<endl;
  cin>>n;
  cout<<"Enter the elements in the array\n";
  for(int i=0;i<n;++i)
    cin>>a[i];
  cout<<"The reversed array is\n";
  for(int i=n-1;i>=0;--i)
    cout<<a[i]<<" ";
  return 0;
}
